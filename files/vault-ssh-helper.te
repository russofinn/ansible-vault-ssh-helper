module vault-ssh-helper 1.0;

require {
	type trivnet1_port_t;
    type var_log_t;
    type sshd_t;
    class file { create open };
    class tcp_socket name_connect;
    class capability2 block_suspend;
}

#============= sshd_t ==============
allow sshd_t self:capability2 block_suspend;
allow sshd_t var_log_t:file create;
allow sshd_t var_log_t:file open;
allow sshd_t trivnet1_port_t:tcp_socket name_connect;
